import java.util.*;

public class JavaBasics{
public static void main(String args[]) {
Scanner scr = new Scanner(System.in);
int income = Scr.nextInt();
int Tax;
if (income < 500000) {
tax=0;
}
else if(income>=500000&& income<=1000000)
{
tax = (int) (income * 0.2);
}
else {
tax = (int) (income * 0.3);
 } 
System.out.printin("your tax is:" + tax);
}
}